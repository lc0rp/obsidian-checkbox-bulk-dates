{
	"name": "obsidian-checkbox-bulk-dates",
	"version": "0.0.1",
	"description": "Adds creation dates to unchecked checkboxes, either retroactively on-command or in real-time. Compatible with Tasks plugin",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint:md": "markdownlint *.md docs/*.md",
		"lint:md:fix": "markdownlint --fix *.md docs/*.md"
	},
	"keywords": [],
	"author": "lc0rp",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"markdownlint-cli": "^0.37.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
